{
	"manifest_version": 2,
	"name": "Okooo Analyzer",
	"version": "1.2.0",
	"description": "A Chrome extension for analyze Okooo data",
	"content_scripts": [
		{
			"matches": [
				"*://www.okooo.com/jingcai/*"
			],
			"run_at": "document_start",
			"all_frames": false,
			"js": [
				"dist/scripts/index.js"
			],
			"css": [
				"dist/stylesheets/index.css"
			]
		}
	],
	"browser_action": {
		"default_popup": "dist/popup-page/index.html",
		"default_icon": {
			"48": "icons/48.png",
			"128": "icons/128.png"
		}
	},
	"icons": {
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": [
		"*://okooo.com/*",
		"storage"
	]
}
